<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="UrlImageView">
        <attr name="square" format="boolean" />
    </declare-styleable>

    <declare-styleable name="OneLineTextInputView">
        <attr name="title_text" format="reference|string"/>
        <attr name="option_text" format="reference|string"/>
        <attr name="button_text" format="reference|string"/>
        <attr name="guide_text" format="reference|string"/>
        <attr name="title_visibility" format="string"/>
        <attr name="option_visibility" format="string"/>
        <attr name="input_text" format="reference|string"/>
        <attr name="input_text_color" format="reference"/>
